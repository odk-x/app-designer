{
    "model": {
        "picture": {
            "type": "image/*"
        }, 
        "visited_seattle": {
            "type": "string"
        }, 
        "name": {
            "type": "string"
        }, 
        "rating": {
            "type": "integer"
        }, 
        "specialTemplateTest": {
            "type": "string"
        }, 
        "pulseox": {
            "type": "string"
        }, 
        "time": {
            "type": "string"
        }, 
        "image_test": {
            "type": "image/*"
        }, 
        "datetime": {
            "type": "string"
        }, 
        "recording": {
            "type": "audio/*"
        }, 
        "born": {
            "type": "string"
        }, 
        "audio_test": {
            "type": "audio/*"
        }, 
        "examples": {
            "type": "string"
        }, 
        "error": {
            "type": "string"
        }, 
        "x": {
            "type": "string"
        }, 
        "video_test": {
            "type": "video/*"
        }, 
        "visited_continents": {
            "type": "string"
        }, 
        "breathcount": {
            "type": "integer"
        }
    }, 
    "settings": [
        {
            "rowNum": 2, 
            "setting": "formId", 
            "value": "exampleForm"
        }, 
        {
            "rowNum": 3, 
            "setting": "formVersion", 
            "value": 1
        }, 
        {
            "rowNum": 4, 
            "setting": "formTitle", 
            "value": "Example Form"
        }
    ], 
    "survey": [
        {
            "name": "examples", 
            "rowNum": 2, 
            "type": "select_multiple", 
            "param": "examples", 
            "label": "Which examples would you like to see?"
        }, 
        {
            "rowNum": 3, 
            "type": "goto", 
            "param": "error_end", 
            "condition": "not(selected(data('examples'), 'error'))"
        }, 
        {
            "name": "error", 
            "hint": "Error prompts are useful for finding bugs in forms with lots of branching.", 
            "rowNum": 4, 
            "type": "select_one", 
            "param": "yesno", 
            "label": "Trigger an error on the next screen?"
        }, 
        {
            "rowNum": 5, 
            "type": "error", 
            "param": null, 
            "condition": "selected(data('error'), 'yes')"
        }, 
        {
            "rowNum": 6, 
            "type": "label", 
            "param": "error_end"
        }, 
        {
            "rowNum": 7, 
            "type": "goto", 
            "param": "intents_end", 
            "condition": "not(selected(data('examples'), 'intents'))"
        }, 
        {
            "name": "barcode", 
            "rowNum": 8, 
            "type": "barcode", 
            "param": null, 
            "label": "Scan a barcode"
        }, 
        {
            "name": "pulseox", 
            "rowNum": 9, 
            "type": "pulseox", 
            "param": null, 
            "label": "Record blood oxygen level"
        }, 
        {
            "name": "breathcount", 
            "rowNum": 10, 
            "type": "breathcounter", 
            "param": null, 
            "label": "Record breath count"
        }, 
        {
            "name": "picture", 
            "rowNum": 11, 
            "type": "image", 
            "param": null, 
            "label": "Take a picture"
        }, 
        {
            "name": "recording", 
            "rowNum": 12, 
            "type": "audio", 
            "param": null, 
            "label": "Make a recording"
        }, 
        {
            "rowNum": 13, 
            "type": "label", 
            "param": "intents_end"
        }, 
        {
            "rowNum": 14, 
            "type": "goto", 
            "param": "dynamic_label_end", 
            "condition": "not(selected(data('examples'), 'dynamic_label'))"
        }, 
        {
            "name": "name", 
            "hint": "Please use your full name", 
            "rowNum": 15, 
            "type": "text", 
            "param": null, 
            "label": "Enter your name"
        }, 
        {
            "rowNum": 16, 
            "type": "note", 
            "param": null, 
            "label": "Hello {{name.value}}"
        }, 
        {
            "rowNum": 17, 
            "type": "label", 
            "param": "dynamic_label_end"
        }, 
        {
            "rowNum": 18, 
            "type": "goto", 
            "param": "datetime_end", 
            "condition": "not(selected(data('examples'), 'datetime'))"
        }, 
        {
            "name": "born", 
            "rowNum": 19, 
            "type": "date", 
            "param": null, 
            "label": "When were you born?"
        }, 
        {
            "param": null, 
            "rowNum": 20, 
            "type": "note", 
            "condition": "data('born').getDay() === now().getDay() && data('born').getMonth() === now().getMonth()", 
            "label": "Happy Birthday!"
        }, 
        {
            "name": "time", 
            "rowNum": 21, 
            "type": "time", 
            "param": null, 
            "label": "What time is it?"
        }, 
        {
            "name": "datetime", 
            "rowNum": 22, 
            "type": "datetime", 
            "param": null, 
            "label": "When will you be available for a follow up survey?"
        }, 
        {
            "rowNum": 23, 
            "type": "label", 
            "param": "datetime_end"
        }, 
        {
            "name": "location", 
            "rowNum": 24, 
            "param": null, 
            "type": "geopoint", 
            "condition": "selected(data('examples'), 'geopoint')", 
            "label": "Record your location:"
        }, 
        {
            "rowNum": 25, 
            "type": "screen", 
            "prompts": [
                {
                    "name": "visited_continents", 
                    "rowNum": 26, 
                    "type": "select", 
                    "param": "continents", 
                    "label": "Which continents have you visited?"
                }, 
                {
                    "name": "visited_seattle", 
                    "rowNum": 27, 
                    "param": "yesno", 
                    "type": "select_one", 
                    "condition": "calculates.ask_about_seattle()", 
                    "label": "Have you visited Seattle?"
                }
            ], 
            "condition": "selected(data('examples'), 'screen_group')"
        }, 
        {
            "rowNum": 29, 
            "type": "goto", 
            "param": "media_end", 
            "condition": "not(selected(data('examples'), 'media'))"
        }, 
        {
            "rowNum": 30, 
            "type": "screen", 
            "prompts": [
                {
                    "name": "audio_test", 
                    "rowNum": 31, 
                    "type": "audio", 
                    "param": null, 
                    "label": "Make a recording:"
                }, 
                {
                    "name": "video_test", 
                    "rowNum": 32, 
                    "type": "video", 
                    "param": null, 
                    "label": "Take a video:"
                }, 
                {
                    "name": "image_test", 
                    "rowNum": 33, 
                    "type": "image", 
                    "param": null, 
                    "label": "Take a picture:"
                }
            ]
        }, 
        {
            "rowNum": 35, 
            "audio": "audio/carrioncrow.mp3", 
            "type": "note", 
            "image": "img/dolphin.png", 
            "param": null, 
            "label": "This prompt shows how to embed media in labels."
        }, 
        {
            "rowNum": 36, 
            "type": "label", 
            "param": "media_end"
        }, 
        {
            "name": "x", 
            "rowNum": 37, 
            "type": "with_next", 
            "assign": 10, 
            "param": null
        }, 
        {
            "param": null, 
            "rowNum": 38, 
            "type": "note", 
            "condition": "selected(data('examples'), 'assignment')", 
            "label": "Assigning a value of 10 to x."
        }, 
        {
            "param": null, 
            "rowNum": 39, 
            "type": "note", 
            "condition": "data('x') === 10", 
            "label": "x = {{x.value}}"
        }, 
        {
            "name": "specialTemplateTest", 
            "param": null, 
            "label": "This is a custom template that uses D3.js:", 
            "rowNum": 40, 
            "type": "text", 
            "condition": "selected(data('examples'), 'custom_template')", 
            "templatePath": "test.handlebars"
        }, 
        {
            "name": "rating", 
            "hint": "1 is very bad. 10 is very good.", 
            "inputAttributes": {
                "max": 10, 
                "type": "range", 
                "min": 1
            }, 
            "param": null, 
            "label": "How would you rate this survey?", 
            "default": 5, 
            "rowNum": 41, 
            "type": "integer"
        }
    ], 
    "calculates": [
        {
            "calculation": "selected(data('visited_continents'), 'NorthAmerica')", 
            "rowNum": 2, 
            "name": "ask_about_seattle"
        }
    ], 
    "choices": {
        "examples": [
            {
                "rowNum": 11, 
                "name": "error", 
                "label": "error"
            }, 
            {
                "rowNum": 12, 
