                "name": "datetime", 
                "label": "datetime"
            }, 
            {
                "rowNum": 15, 
                "name": "geopoint", 
                "label": "geopoint"
            }, 
            {
                "rowNum": 16, 
                "name": "media", 
                "label": "media"
            }, 
            {
                "rowNum": 17, 
                "name": "screen_group", 
                "label": "screen group with select and calculate"
            }, 
            {
                "rowNum": 18, 
                "name": "custom_template", 
                "label": "custom template"
            }, 
            {
                "rowNum": 19, 
                "name": "assignment", 
                "label": "assignment"
            }
        ], 
        "yesno": [
            {
                "rowNum": 2, 
                "name": "yes", 
                "label": "yes"
            }, 
            {
                "rowNum": 3, 
                "name": "no", 
                "label": "no"
            }
        ], 
        "continents": [
            {
                "rowNum": 4, 
                "name": "NorthAmerica", 
                "label": "North America"
            }, 
            {
                "rowNum": 5, 
                "name": "SouthAmerica", 
                "label": "South America"
            }, 
            {
                "rowNum": 6, 
                "name": "Europe", 
                "label": "Europe"
            }, 
            {
                "rowNum": 7, 
                "name": "Africa", 
                "label": "Africa"
            }, 
            {
                "rowNum": 8, 
                "name": "Asia", 
                "label": "Asia"
            }, 
            {
                "rowNum": 9, 
                "name": "Australia", 
                "label": "Australia"
            }, 
            {
                "rowNum": 10, 
                "name": "Antarctica", 
                "label": "Antarctica"
            }
        ]
    }
}