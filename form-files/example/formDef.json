        }, 
        {
            "name": "born", 
            "_rowNum": 22, 
            "type": "date", 
            "param": null, 
            "label": "When were you born?"
        }, 
        {
            "param": null, 
            "_rowNum": 23, 
            "type": "note", 
            "condition": "data('born').getDay() === now().getDay() && data('born').getMonth() === now().getMonth()", 
            "label": "Happy Birthday!"
        }, 
        {
            "name": "time", 
            "_rowNum": 24, 
            "type": "time", 
            "param": null, 
            "label": "What time is it?"
